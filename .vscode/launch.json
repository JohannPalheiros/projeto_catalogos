{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: Executar Arquivo Atual",
        "type": "debugpy",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal",
        "env": {
          "PYTHONPATH": "${workspaceFolder}"
        },
        "justMyCode": true
      },
      {
        "name": "Python: Debug Batch Processor",
        "type": "debugpy",
        "request": "launch",
        "module": "src.batch_processor",
        "console": "integratedTerminal",
        "env": {
          "PYTHONPATH": "${workspaceFolder}"
        }
      }
    ]
  }
  